{
	"name": "CSCI-3130 Project",
	"description": "Build an Extension that downloads media from a website.",
	"version": "0.1",
	"manifest_version": 3,
	"background": {
		"service worker" : "background.js"
	},
	"icons": {
		"128": "icons/puzzle.png"
	},
	"permissions": ["storage"],
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"128":"icons/puzzle.png"
		}
	},
	"content_scripts" : [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["content.js"]
		}
	]
}