{
    "name":"DownLoad Extension",
    "description": "Test",
    "version": "2.0",
    "manifest_version": 2,
    "icons":{
        "16":"./logo16.png",
        "32":"./logo32.png",
        "48":"./logo48.png",
        "128":"./logo128.png"
    },
    "background":{
        "scripts":["./background.js"]
    },
    "options_page":"./options.html",
    "browser_action":{
        "default_popup": "popup.html", 
        "default_options":"options.html"
    },
    "permissions":[
        "tabs",
        "https://www.google.com/*"
    ]
}
